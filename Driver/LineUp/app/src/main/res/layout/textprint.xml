<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/widget28"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <TableRow
        android:id="@+id/widget29"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="                   Print data："></TextView>

        <EditText
            android:id="@+id/text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0123456789ABCDEFG"
            android:textSize="18sp"></EditText>
    </TableRow>

    <TableRow
        android:id="@+id/widget29"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="                     Reverse："></TextView>

        <ToggleButton
            android:id="@+id/text_StyleReverse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ToggleButton" />

    </TableRow>

    <TableRow
        android:id="@+id/widget30"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="                           Bold："></TextView>

        <ToggleButton
            android:id="@+id/text_StyleBold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ToggleButton" />

    </TableRow>

    <TableRow
        android:id="@+id/widget31"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget40"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="                  Underline："></TextView>

        <ToggleButton
            android:id="@+id/text_StyleUnderline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ToggleButton" />

    </TableRow>

    <TableRow
        android:id="@+id/widget32"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget41"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="                   Font type："></TextView>

        <Spinner
            android:id="@+id/text_fonttype_spi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Spinner>
    </TableRow>

    <TableRow
        android:id="@+id/widget56"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget41"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="         Alignment type："></TextView>

        <Spinner
            android:id="@+id/text_alignmenttype_spi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Spinner>
    </TableRow>

    <TableRow
        android:id="@+id/widget55"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget56"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Horstarting position："></TextView>

        <EditText
            android:id="@+id/text_HorStartingPosition"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:text="100"
            android:textSize="18sp"></EditText>
    </TableRow>

    <TableRow
        android:id="@+id/widget34"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget44"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Verstarting position："></TextView>

        <EditText
            android:id="@+id/text_VerStartingPosition"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:text="20"
            android:textSize="18sp"></EditText>
    </TableRow>

    <TableRow
        android:id="@+id/widget33"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget42"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="               Line height："></TextView>

        <EditText
            android:id="@+id/text_LineHeight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:text="10"
            android:textSize="18sp"></EditText>
    </TableRow>

    <TableRow
        android:id="@+id/widget36"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget45"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="      Horizontal times："></TextView>

        <Spinner
            android:id="@+id/text_hortimes_spi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Spinner>
    </TableRow>

    <TableRow
        android:id="@+id/widget37"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget47"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="           Vertical times："></TextView>

        <Spinner
            android:id="@+id/text_vertimes_spi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Spinner>
    </TableRow>

    <TableRow
        android:id="@+id/widget46"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/text_print_but"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Print" />

        <Button
            android:id="@+id/text_ret_but"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Back" />

    </TableRow>
</LinearLayout>
