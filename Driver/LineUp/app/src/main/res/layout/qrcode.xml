<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/widget28"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <TableRow
        android:id="@+id/widget29"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="                    Print data："></TextView>

        <EditText
            android:id="@+id/qr_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="QA,123456789ABCDEFG"
            android:textSize="18sp"></EditText>
    </TableRow>

    <TableRow
        android:id="@+id/widget30"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="HorStarting position："></TextView>

        <EditText
            android:id="@+id/qr_posit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:text="0"
            android:textSize="18sp"></EditText>
    </TableRow>

    <TableRow
        android:id="@+id/widget31"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Basic element width："></TextView>

        <Spinner
            android:id="@+id/qr_mode_width_spi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Spinner>
    </TableRow>

    <TableRow
        android:id="@+id/widget32"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="             Symbol type："></TextView>

        <Spinner
            android:id="@+id/qr_boltype_spi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Spinner>
    </TableRow>

    <TableRow
        android:id="@+id/widget33"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/widget39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="       Language mode："></TextView>

        <Spinner
            android:id="@+id/qr_language_spi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></Spinner>
    </TableRow>

    <TableRow
        android:id="@+id/widget34"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/qr_print_but"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Print" />

        <Button
            android:id="@+id/qr_ret_but"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Back" />

    </TableRow>
</LinearLayout>
